keymap = NULL #INFO
# keymap > SHIFT_RIGHT
keymap ? NULL #SEARCH_BACK
keymap @ NULL #READ_SHELL
keymap B NULL #BACK
keymap D NULL #DOWNLOAD_LIST
# keymap DOWN MOVE_DOWN
# keymap UP MOVE_UP
# keymap RIGHT MOVE_RIGHT
# keymap LEFT MOVE_LEFT
keymap E NULL #EDIT
# keymap ESC-: MARK_MID
keymap ESC-C-j NULL #SAVE_LINK
