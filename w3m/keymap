keymap q NULL #QUIT
keymap r NULL #VERSION
keymap s NULL #SELECT_MENU
keymap u NULL #PEEK_LINK
keymap v NULL #VIEW
# keymap w NEXT_WORD
keymap z NULL #CENTER_V
keymap { NULL #PREV_TAB
keymap | NULL #PIPE_BUF
keymap } NULL #NEXT_TAB
# }}}

########## command ##########
keymap :: COMMAND
keymap :h HELP
keymap :so SET_OPTION
keymap :| PIPE_BUF

########## history navigation ##########
keymap L NEXT
keymap H PREV
keymap :h SELECT_MENU
#keymap :h HISTORY

########## scrolling ##########
keymap j UP
keymap k DOWN
keymap C-e DOWN
keymap C-y UP
keymap gg BEGIN
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap zz CENTER_V

########## cursor ##########
keymap J MOVE_DOWN
keymap K MOVE_UP
keymap 0 LINE_BEGIN
keymap b PREV_WORD

########## cursor history ##########
keymap C-i REDO
keymap C-o UNDO

########## navigation ##########
keymap f MOVE_LIST_MENU
keymap F LIST_MENU
keymap C-n NEXT_LINK
keymap C-p PREV_LINK
keymap i VIEW_IMAGE
keymap I SAVE_IMAGE

# reload
keymap r RELOAD

# save/load 
keymap :w SAVE
keymap :W SAVE_SCREEN
keymap :p PRINT
keymap :o GOTO
keymap :e LOAD

keymap :r READ_SHELL

# jump
keymap ESC-C-j SUBMIT
keymap gf TAB_LINK
keymap C-] GOTO_LINK
keymap SPC GOTO_LINK
########## info ##########
keymap y PEEK
keymap ;y PEEK_LINK
keymap gC-g INFO

########## search ##########
keymap / ISEARCH
keymap ? ISEARCH_BACK

########## bookmarks ##########
keymap B ADD_BOOKMARK
keymap :b VIEW_BOOKMARK

keymap m MARK

########## tab ##########
keymap d CLOSE_TAB
keymap gh GOTO
keymap gH TAB_GOTO
keymap C-t NEW_TAB
keymap gt NEXT_TAB
keymap gT PREV_TAB
keymap C-wl TAB_RIGHT
keymap C-wh TAB_LEFT
keymap t TAB_GOTO
keymap T TAB_MENU

########## quit ##########
keymap :q QUIT 

